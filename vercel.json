{
  "version": 2,
  "buildCommand": "node build.js",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "functions": {
    "server/index.ts": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ]
}